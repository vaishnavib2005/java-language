import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int sum=0;
	    a=Math.abs(a);
	    while(a>0){
	        int digit=a%10;
	        sum+=digit;
	        a=a/10;
	    }
		System.out.println("Sum of the digits"+" "+sum);
	}
}
